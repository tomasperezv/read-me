{
  "manifest_version": 2,
  "name": "read-me",
  "description": "Text to voice webpages via the Speech Synthesis API",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "read-me.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
